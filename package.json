{
  "name": "@iqprotocol/iq-space-sdk-js",
  "version": "2.1.12",
  "bugs": "https://github.com/iqlabsorg/iq-space-sdk-js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/iqlabsorg/iq-space-sdk-js"
  },
  "license": "MIT",
  "author": "IQ Labs (https://iq.space)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "NODE_OPTIONS=--max-old-space-size=5120 rollup -c",
    "build:clean": "rimraf dist",
    "prepublish": "NODE_ENV=production yarn build",
    "start": "rollup -cw",
    "test": "cross-env LOG_LEVEL=error TS_NODE_IGNORE=\"/node_modules/(?!@iqprotocol/iq-space-protocol/deploy)/\" ts-mocha test/**/*.test.ts",
    "docs": "run-s docs:clean docs:generate",
    "docs:generate": "NODE_OPTIONS=--max_old_space_size=8192 typedoc --options typedoc.js --entryPointStrategy packages . --out docs && touch docs/.nojekyll",
    "docs:clean": "rimraf docs",
    "cache:clean": "rimraf cache",
    "logs:clean": "rimraf logs && rimraf tslog.log",
    "deploy:clean": "rimraf hardhat.addresses.json && rimraf active-deployment",
    "coverage": "nyc yarn test",
    "coverage:clean": "rimraf coverage && rimraf .nyc_output",
    "clean": "run-s build:clean cache:clean docs:clean logs:clean deploy:clean coverage:clean",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint --fix **/*.ts"
  },
  "lint-staged": {
    "**/*.ts": "yarn lint:fix",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@iqprotocol/iq-space-protocol-light": "2.1.13",
    "caip": "^1.1.0"
  },
  "devDependencies": {
    "@iqprotocol/iq-space-protocol": "2.1.13",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@knodes/typedoc-plugin-monorepo-readmes": "^0.22.5",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@latest",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@swc/core": "^1.3.19",
    "@swc/helpers": "^0.4.14",
    "@typechain/ethers-v5": "10.0.0",
    "@typechain/hardhat": "^6.1.4",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "ethers": "^5.6.0",
    "hardhat": "^2.6.5",
    "hardhat-deploy": "^0.11.10",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-swc": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^3.0.1",
    "sort-package-json": "^1.52.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.2.1",
    "typechain": "8.0.0",
    "typedoc": "0.22.17",
    "typescript": "^4.7.0"
  },
  "peerDependencies": {
    "ethers": "^5.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "typedocMain": "src/index.ts"
}
